<div id="card">
<%= form_for @group, html: {multipart: true, "data-ajax" => false} do |f| %>
	<p>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</p>

	<p>
		<%= f.label :description %>
		<%= f.text_area :description %>
	</p>

	<p>
		<%= f.label :icon %>
		<%= f.file_field :icon %>
	</p>

	<p>
		<%= f.label :private %>
		<%= f.check_box :private %>
	</p>
	
	<p>
		<%= f.submit "Create Group" %>
	</p>
<% end %>
</div>