<%= button_to "Start a group", new_group_path, method: :get %><br>
	

<% @groups.each do |group| %>
	<div id="card">
		<table align="center">
			<tr>
				<td><%= link_to image_tag(group.icon, size: "50x50"), group_path(group) %></td>
				<td><h2><%= link_to group.name, group_path(group) %></h2></td>
			</tr>
		</table>
		<div align="center">Members: <%= group.members.size %></div>
	</div>
	<br>
<% end %>