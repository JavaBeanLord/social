<%= button_to "Start a group", new_group_path, method: :get %><br>
	

<% if @groups.size > 0 %>
		<% @groups.each do |group| %>
			<div id="card">
				<div align="center"><%= link_to image_tag(group.icon, size: "100x100"), group_path(group) %></div>
				<div align="center"><%= link_to group.name, group_path(group) %></div>
			</div>
		<% end %>
<% end %>