<%= form_for @user do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
 
  <p>
    <%= f.label :bio %><br>
    <%= f.text_area :bio %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>