<!-- social welcome -->

<h1>Welcome to Social</h1>

<p>
	<%= link_to "List users", users_path %>
</p>

<% if current_user %>
<h2>Feed:</h2>

<table>
	<% @feed.each do |post| %>
	<tr>
		<td><%= gravatar_for User.find(post.user_id) %></td>
		<td><%= post.text %></td>
	</tr>
	<% end %>
</table>
<% end %>